clear      lock
clear   control
clear      mod1
clear      mod2
clear      mod3
clear      mod4
clear      mod5
!! Caps_Lock key -> Hyper_L
keycode      66 = Hyper_L
!! If you want Control_R, It can be found on R_Alt in addition to Control_R
keycode	    108 = Control_R
keycode	    105 = Control_R
!! If you want Caps_Lock, find it on Pause
keycode	    127 = Caps_Lock
add     control = Control_L Control_R
add        mod1 = Alt_L Alt_R Meta_L
add        mod2 = Num_Lock
add        mod3 = Hyper_L
add        mod4 = Super_L Super_R
add        mod5 = Mode_switch ISO_Level3_Shift
